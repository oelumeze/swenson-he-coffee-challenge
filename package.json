{
  "name": "swenson-he-coffee-challenge",
  "version": "1.0.0",
  "description": "Swenson HE BackEnd Challenge",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "dev": "nodemon -w src --exec babel-node --presets env src/server.js",
    "start":  "npm run build && node ./build/server.js",
    "test": "./node_modules/.bin/mocha --require babel-core/register src/tests/*.spec.js"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Ogo Elumeze",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babel-preset-env": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "mongo": "^0.1.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.5",
    "save-dev": "^2.0.0"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2"
  }
}
